{
	"manifest_version": 2,
	"applications": {
		"gecko": {
			"id": "download-filename-encoding@qw.linux-2g64.local"
		}
	},
	"name": "__MSG_extensionName__",
	"version": "2.0",
	"description": "__MSG_extensionDescription__",
	"permissions": [
		"<all_urls>",
		"storage",
		"webRequest",
		"webRequestBlocking",
		"menus"
	],
	"background": {
		"page": "background.html"
	},
	"options_ui": {
		"page": "options.html"
	},
	"default_locale": "en",
	"icons": {
		"48": "icons/icon.svg"
	},
	"browser_action": {
		"default_icon": "icons/icon.svg",
		"default_title": "__MSG_extensionName__",
		"theme_icons": [
			{
				"light": "icons/icon-light.svg",
				"dark": "icons/icon.svg",
				"size": 48
			}
		]
	}
}